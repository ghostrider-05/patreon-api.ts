{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["../src/index.ts"],
    "tsconfig": "./typedoc.tsconfig.json",
    "readme": "none",
    "out": "api",
    "excludeProtected": true,
    "excludeInternal": true,
    "excludePrivate": true,
    "includeVersion": true,
    "cleanOutputDir": true,
    "hideGenerator": true,
    "hideBreadcrumbs": true,
    "githubPages": false,
    "maxTypeConversionDepth": 50,
    "treatWarningsAsErrors": true,
    "treatValidationWarningsAsErrors": true,
    // Markdown options
    "expandObjects": true,
    "parametersFormat": "table",
    "classPropertiesFormat": "table",
    "interfacePropertiesFormat": "table",
    "tableColumnSettings": {
        "hideSources": true,
    },
    "notRenderedTags": [
        "@showCategories",
        "@showGroups",
        "@hideCategories",
        "@hideGroups",
        "@disableGroups",
        "@expand",
        "@preventExpand",
        "@expandType",
        "@summary",
        "@group",
        "@groupDescription",
        "@category",
        "@categoryDescription",
        // custom tags
        "@format"
    ],
    "blockTags": [
        "@defaultValue",
        "@deprecated",
        "@example",
        "@param",
        "@privateRemarks",
        "@remarks",
        "@returns",
        "@see",
        "@throws",
        "@typeParam",
        "@author",
        "@callback",
        "@category",
        "@categoryDescription",
        "@default",
        "@document",
        "@extends",
        "@augments",
        "@yields",
        "@group",
        "@groupDescription",
        "@import",
        "@inheritDoc",
        "@jsx",
        "@license",
        "@module",
        "@mergeModuleWith",
        "@prop",
        "@property",
        "@return",
        "@satisfies",
        "@since",
        "@template",
        "@type",
        "@typedef",
        "@summary",
        "@preventInline",
        "@inlineType",
        "@preventExpand",
        "@expandType",
        // Custom blocks
        "@format",
        "@constant"
    ],
    "intentionallyNotDocumented": [],
    "intentionallyNotExported": [
        // utility types
        "If",
        "IfAsync",
        "Additional",
        "StringObject",
        // relationship types
        "RelationDataItem",
        "ResolvedRelation",
        "RelationshipItemProperty",
        // schema types
        "ModifiableResourceAttributes",
        "ModifiableResourceRelationships",
        "PaginationQuerySort",
        // client types
        "RestClient",
        "PatreonClientMethods",
        "GenericPatreonClientMethods",
        "OauthClientOptions",
        "Oauth2RedirectOptions",
        // payload types
        "RequestPayload",
        "PayloadFromQuery",
        "GetRequestType",
        "GetRequestPayload",
        "ListRequestPaginationPayload",
        // simplified types
        "AnyToCamelCase",
        "ListRequestPayload",
        "NormalizedRequestPayload",
        "NormalizedListRequestPayload",
        "NormalizeRequest",
        "GetNormalizedResponsePayload",
        // Mock data types
        "RandomDataGenerator",
        "PatreonMockDataRandomResources",
        "PatreonMockWebhooks",
        "PatreonMockWebhooksOptions",
        // deprecated types
        "PatreonFetchOptions",
        "PatreonMemoryTokenStore",
        "PatreonKVTokenStore",
        "PatreonFetchTokenStore"
    ],
    "plugin": [
        "typedoc-plugin-markdown",
        "typedoc-vitepress-theme"
    ]
}