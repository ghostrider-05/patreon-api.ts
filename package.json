{
  "name": "patreon-api.ts",
  "version": "0.2.0",
  "description": "Typescript library for the Patreon API",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "scripts": {
    "prestart": "npx tsc && npx gen-esm-wrapper ./dist/index.js ./dist/index.mjs",
    "test": "vitest --typecheck",
    "test:coverage": "vitest run --typecheck --coverage",
    "lint": "npx eslint --fix --ext .ts src/",
    "publish": "npm run test && npm run prestart && npm run lint && npm install && npm publish"
  },
  "engines": {
    "node": ">=18.17"
  },
  "homepage": "https://github.com/ghostrider-05/patreon-api.ts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghostrider-05/patreon-api.ts.git"
  },
  "bugs": {
    "url": "https://github.com/ghostrider-05/patreon-api.ts/issues/"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "typescript",
    "patreon",
    "patreon-api"
  ],
  "author": {
    "name": "ghostrider-05"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.19.31",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitest/coverage-istanbul": "^1.4.0",
    "eslint": "^8.57.0",
    "gen-esm-wrapper": "^1.1.3",
    "typescript": "^5.4.4",
    "vitest": "1.4.0"
  },
  "dependencies": {
    "client-oauth2": "^4.3.3"
  }
}
