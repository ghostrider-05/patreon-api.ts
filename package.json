{
  "name": "patreon-api.ts",
  "version": "0.7.0",
  "description": "Typescript library for the V2 Patreon API",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prestart": "npx tsc && node ./tsc-generated/src/schemas/v2/scripts/index.js",
    "build": "tsup",
    "test": "vitest --typecheck",
    "test:coverage": "vitest run --typecheck --coverage",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint --fix src/**/*.ts"
  },
  "engines": {
    "node": ">=18.17"
  },
  "homepage": "https://github.com/ghostrider-05/patreon-api.ts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghostrider-05/patreon-api.ts.git"
  },
  "bugs": {
    "url": "https://github.com/ghostrider-05/patreon-api.ts/issues/"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "patreon",
    "patreon-api",
    "patreon-wrapper",
    "patreon-client",
    "typescript",
    "oauth",
    "webhook-server"
  ],
  "author": {
    "name": "ghostrider-05"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/node": "^18.19.31",
    "@vitest/coverage-istanbul": "^2.0.5",
    "eslint": "^9.10.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "ts-morph": "^23.0.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vitest": "2.0.5"
  }
}
