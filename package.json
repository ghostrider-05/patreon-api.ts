{
  "name": "patreon-api.ts",
  "version": "0.6.0",
  "description": "Typescript library for the V2 Patreon API",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "scripts": {
    "prestart": "npx tsc && npx gen-esm-wrapper ./dist/index.js ./dist/index.mjs",
    "test": "vitest --typecheck",
    "test:coverage": "vitest run --typecheck --coverage",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint --fix src/**/*.ts"
  },
  "engines": {
    "node": ">=18.17"
  },
  "homepage": "https://github.com/ghostrider-05/patreon-api.ts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghostrider-05/patreon-api.ts.git"
  },
  "bugs": {
    "url": "https://github.com/ghostrider-05/patreon-api.ts/issues/"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "typescript",
    "patreon",
    "patreon-api"
  ],
  "author": {
    "name": "ghostrider-05"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/node": "^18.19.31",
    "@vitest/coverage-istanbul": "^1.6.0",
    "eslint": "^9.5.0",
    "eslint-plugin-jsdoc": "^48.4.0",
    "gen-esm-wrapper": "^1.1.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.13.1",
    "vitest": "1.6.0"
  }
}
